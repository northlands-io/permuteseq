{
    "name": "northlands/permuteseq",
    "description": "A small PHP library to obfuscate numbers.",
    "keywords": [
        "permutation",
        "obfuscation",
        "fiestel"
    ],
    "homepage": "https://github.com/northlands/permuteseq",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Skovvang Petersen",
            "email": "martin@northlands.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^1.21",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Northlands\\Permuteseq\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Northlands\\Permuteseq\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
